{
  "name": "chordrectness",
  "version": "1.0.0",
  "description": "YouTube Chord Recognition Platform - Enterprise-grade monorepo",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*",
    "services/*"
  ],
  "scripts": {
    "dev": "docker-compose -f docker-compose.yml up --build",
    "dev:down": "docker-compose -f docker-compose.yml down",
    "dev:logs": "docker-compose -f docker-compose.yml logs -f",
    "build": "turbo run build",
    "test": "turbo run test",
    "type-check": "turbo run type-check",
    "release": "turbo run build --filter=!@chordrectness/docs"
  },
  "devDependencies": {
    "turbo": "^1.11.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "chord-recognition",
    "music-analysis",
    "youtube",
    "audio-processing",
    "jazz",
    "music-theory",
    "typescript",
    "nextjs",
    "fastify",
    "monorepo"
  ],
  "author": "ChordRectness Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/chordrectness.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/chordrectness/issues"
  },
  "homepage": "https://chordrectness.com"
}